{
  "name": "defer-map",
  "version": "1.0.0",
  "description": "A small Map wrapper with defer and expiry.",
  "keywords": [
    "map",
    "defer",
    "promise",
    "expiration",
    "expiry"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/defer-map",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/defer-map"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/defer-map/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "build": "tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "format": "prettier --write **/*.{js,ts}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@justinlettau/renovate-config": "^0.1.2",
    "@justinlettau/tslint-config": "^0.1.0",
    "@types/jasmine": "^3.5.10",
    "codecov": "^3.6.5",
    "husky": "^4.2.3",
    "jasmine": "^3.5.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "standard-version": "^7.1.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
